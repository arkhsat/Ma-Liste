{
  "expo": {
    "name": "Ma-Liste",
    "slug": "Ma-Liste",
    "scheme": "MaListe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MaList4.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/MaList2.png",
      "resizeMode": "contain",
      "backgroundColor": "#161622"
    },
    "ios": {
      "supportsTablet": true,
      "package": "com.jsm.malist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jsm.malist"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c444a85e-34a0-42fd-9f55-3d83202ecd01"
      }
    }
  }
}
